<trans-unit id="0fea5eb0-3941-4bcb-bff2-bf37fdf294c8"><source><g id="39">    &lt;match target="font"&gt;
        &lt;test compare="more" name="size" qual="any" &gt;
            &lt;double&gt;0&lt;/double&gt;
        &lt;/test&gt;
        &lt;test compare="less" name="size" qual="any" &gt;
            &lt;double&gt;72&lt;/double&gt;
        &lt;/test&gt;
        &lt;edit mode="assign" name="antialias" &gt;
            &lt;bool&gt;false&lt;/bool&gt;
        &lt;/edit&gt;
    &lt;/match&gt;
</g></source><seg-source><g id="39">    <mrk mtype="seg" mid="28">&lt;match target="font"&gt;
        &lt;test compare="more" name="size" qual="any" &gt;
            &lt;double&gt;0&lt;/double&gt;
        &lt;/test&gt;
        &lt;test compare="less" name="size" qual="any" &gt;
            &lt;double&gt;72&lt;/double&gt;
        &lt;/test&gt;
        &lt;edit mode="assign" name="antialias" &gt;
            &lt;bool&gt;false&lt;/bool&gt;
        &lt;/edit&gt;
    &lt;/match&gt;</mrk>
</g></seg-source><target><g id="39">    <mrk mtype="seg" mid="28" />
</g></target><sdl:seg-defs><sdl:seg id="28" origin="not-translated" /></sdl:seg-defs></trans-unit>
